<Window x:Class="WCFSelfHost.Client_ClientFactory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WCFSelfHost.Client_ClientFactory"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel Margin="20">
        <WrapPanel DockPanel.Dock="Top">
            <Button Click="Laden">TCP Laden</Button>
            <Button Click="LadenBasic">Basic Laden</Button>
            <Button Click="LadenWS">WS Laden</Button>
            <Button Click="LadenIPC">IPC Laden</Button>
            
            <Button Click="AddAuto">Add</Button>
            <Button Click="AddAutoFail">Add FAULT</Button>
        </WrapPanel>
        <DataGrid x:Name="myGrid"/>
    </DockPanel>

</Window>
